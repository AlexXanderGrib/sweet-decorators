{
  "name": "sweet-decorators",
  "version": "1.3.0",
  "description": "Library of utility typescript decorators",
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "test": "./src/tests",
    "lib": "./dist",
    "doc": "./docs"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc --project ./tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/sweet-decorators.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "decorators",
    "mixin",
    "di",
    "hooks",
    "dependency injection",
    "method hooks",
    "error handling",
    "meta"
  ],
  "author": "AlexXanderGrib <me@alexxgrib.me> (https://alexxgrib.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/sweet-decorators/issues",
    "email": "me@alexxgrib.me"
  },
  "homepage": "https://github.com/AlexXanderGrib/sweet-decorators#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettify",
      "pre-push": "npm test"
    }
  }
}
